<Window x:Class="Hemuppgift_WPFtemplate.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Button Grid.Row="0" Grid.Column="0" Content="Grid" x:Name="btnGrid" Click="btnGrid_Click"  />
        <Button Grid.Row="0" Grid.Column="1" Content="StackPanel" x:Name="btnStackPanel"  Click="btnStackPanel_Click" />
        <Button Grid.Row="1" Grid.Column="0" Content="WrapPanel" x:Name="btnWrapPanel"  Click="btnWrapPanel_Click" />
        <Button Grid.Row="1" Grid.Column="1" Content="DockPanel" x:Name="btnDockPanel" Click="btnDockPanel_Click" />
    </Grid>
</Window>
