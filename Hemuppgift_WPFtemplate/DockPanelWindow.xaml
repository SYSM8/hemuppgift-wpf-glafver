<Window x:Class="Hemuppgift_WPFtemplate.DockPanelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hemuppgift_WPFtemplate"
        mc:Ignorable="d"
        Title="DockPanelWindow" Height="450" Width="800">
    <DockPanel x:Name="MainDockPanel" LastChildFill="True">
        <Button Content="Top" DockPanel.Dock="Top" Width="80" Height="30" Margin="5"/>
        <Button Content="Left" DockPanel.Dock="Left" Width="80" Height="30" Margin="5" />
        <Button Content="Right" DockPanel.Dock="Right" Width="80" Height="30" Margin="5"/>
        <Button Content="Bottom" DockPanel.Dock="Bottom" Width="80" Height="30" Margin="5"/>
        <Button Content="Center" Width="80" Height="30" Margin="5" x:Name="btnCenter" Click="btnCenter_Click"/>
    </DockPanel>
</Window>
